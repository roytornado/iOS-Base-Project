source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '12.0'

def base_pods
  use_frameworks!

  pod 'Alamofire'
  pod 'Kingfisher'
  pod 'SwiftyJSON'
  pod 'RSLoadingView'
  pod "Flow-iOS"
  pod 'RxSwift', '6.0.0-rc.2'
  pod 'RxCocoa', '6.0.0-rc.2'
  pod 'Cleanse'
end

target "BaseProject" do
  base_pods
end

